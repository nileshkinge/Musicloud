{
  "name": "express-serverless-template",
  "version": "1.0.0",
  "description": "ðŸš€ Template for creating Serverless Node + TypeScript + Express projects",
  "license": "MIT",
  "author": "nilesh kinge",
  "main": "server.js",
  "repository": "https://github.com/nileshkinge/express-serverless-template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "npm run build && cross-env NODE_ENV=dev node dist/src/server.js --watch",
    "build": "tsc",
    "pretty": "prettier --write \"./**/*.{js,ts,ejs,json}\"",
    "lint": "eslint . --ext \".ts\"",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "deploy": "vercel deploy --prod",
    "tailwind:css": "postcss assets/styles/tailwind.css -o assets/styles/style.css"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-timeout-handler": "^2.2.2",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "joi": "^17.6.2",
    "mongoose": "^6.6.3",
    "morgan": "^1.10.0",
    "postcss-cli": "^10.1.0",
    "tslib": "^2.4.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.23",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.24.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
